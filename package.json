{
  "name": "usfl",
  "version": "3.0.0",
  "description": "A collection of tested, reusable JS utilities and snippets.",
  "keywords": [
    "lib",
    "utility",
    "util",
    "utilities",
    "javascript",
    "js"
  ],
  "author": "ianmcgregor",
  "license": "MIT",
  "main": "dist/usfl.min.js",
  "scripts": {
    "postinstall": "babel src --out-dir ./",
    "start": "rollup -c -w",
    "build": "NODE_ENV=production rollup -c && rollup -c",
    "test": "eslint 'src/**/*.js' && karma start --single-run --browsers Firefox",
    "lint": "eslint 'src/**/*.js'; exit 0"
  },
  "readmeFilename": "README.md",
  "repository": {
    "type": "git",
    "url": "https://github.com/ianmcgregor/usfl"
  },
  "dependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "events": "^1.1.1",
    "mini-signals": "^1.2.0"
  },
  "devDependencies": {
    "babel-eslint": "^8.0.0",
    "babelify": "^7.3.0",
    "browserify": "^14.4.0",
    "chai": "^4.1.2",
    "chalk": "^2.1.0",
    "karma": "^1.7.1",
    "karma-browserify": "^5.1.1",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-firefox-launcher": "^1.0.1",
    "karma-mocha": "^1.3.0",
    "mocha": "^3.5.3",
    "rollup": "^0.50.0",
    "rollup-plugin-babel": "^3.0.2",
    "rollup-plugin-commonjs": "^8.2.1",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-plugin-strip": "^1.1.1",
    "rollup-plugin-uglify": "^2.0.1",
    "rollup-watch": "^4.3.1",
    "watchify": "^3.9.0"
  },
  "browserify": {
    "transform": [
      "babelify"
    ]
  }
}
